#!/bin/bash
echo "Pr√©pation pour fork..."
mkdir info
mv LICENSE info/
mv README.md info/
cd file/
unzip file-respinguy.zip
unzip mkinitramfs-respinguy.zip
cd ..
mv file/file-respinguy .
mv file/mkinitramfs-respinguy .
mv file/respinguy-github.zip .
