cp file-respinguy/ResPinGuy-File/respinguy-app-rm ./file-respinguy/ResPinGuy-File/
cp file-respinguy/ResPinGuy-File/respinguy-app-up ./file-respinguy/ResPinGuy-File/
cp -r respinguy-github/fork/ file-respinguy/ResPinGuy-File/
cp respinguy-github/fork/Install-ResPinGuy .
cp respinguy-github/fork/MAJ-ResPinGuy .
